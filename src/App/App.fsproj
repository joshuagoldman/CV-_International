<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <UseLocalSutil>False</UseLocalSutil>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="About\types.fs" />
    <Compile Include="About\funtions\miscellaneous.fs" />
    <Compile Include="About\state.fs" />
    <Compile Include="About\view.fs" />
    <Compile Include="Main\types.fs" />
    <Compile Include="Main\funtions\miscellaneous.fs" />
    <Compile Include="Main\state.fs" />
    <Compile Include="Main\view.fs" />
    <Compile Include="App.fs" />
  </ItemGroup>
  <Choose>
    <When Condition=" '$(UseLocalSutil)'=='True' ">
        <ItemGroup>
            <ProjectReference Include="..\..\..\Sutil\src\Sutil\Sutil.fsproj" />
        </ItemGroup>
    </When>
    <When Condition=" '$(UseLocalSutil)'=='False' ">
        <ItemGroup>
            <PackageReference Include="Sutil" Version="1.0.0-*" />
        </ItemGroup>
    </When>
  </Choose>
</Project>